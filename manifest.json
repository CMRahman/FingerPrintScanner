{
  "manifest_version": 2,
  "name": "BT - version 1",
  "version": "1",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": ["storage"],
  "bluetooth": {
      "uuids": ["1101","112f", "1105", "1106"],
      "socket": true,
      "low_energy": true,
      "peripheral": true
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "minimum_chrome_version": "28"
}
